  <DataSets>
    <DataSet Name="CustomerInvoice_DSet">
      <Query>
        <DataSourceName>DeliveryServices_Shared_DS</DataSourceName>
        <CommandText>SELECT
  Customer.Name
  ,Customer.BillingCity
  ,InvoiceHeader.InvoiceNumber
  ,InvoiceHeader.InvoiceDate
  ,InvoiceHeader.TotalAmount
FROM
  Customer
  LEFT OUTER JOIN InvoiceHeader
    ON Customer.CustomerNumber = InvoiceHeader.CustomerNumber</CommandText>
        <rd:DesignerState>
          <QueryDefinition xmlns="http://schemas.microsoft.com/ReportingServices/QueryDefinition/Relational">
            <AutoJoin>False</AutoJoin>
            <SelectedColumns>
              <ColumnExpression ColumnOwner="Customer" ColumnName="Name" />
              <ColumnExpression ColumnOwner="Customer" ColumnName="BillingCity" />
              <ColumnExpression ColumnOwner="InvoiceHeader" ColumnName="InvoiceNumber" />
              <ColumnExpression ColumnOwner="InvoiceHeader" ColumnName="InvoiceDate" />
              <ColumnExpression ColumnOwner="InvoiceHeader" ColumnName="TotalAmount" />
            </SelectedColumns>
            <Joins>
              <Join Type="CROSS JOIN" ColumnOwner="Customer" />
              <Join Type="LEFT OUTER JOIN" ColumnOwner="InvoiceHeader">
                <On>
                  <From ColumnOwner="Customer" ColumnName="CustomerNumber" />
                  <To ColumnOwner="InvoiceHeader" ColumnName="CustomerNumber" />
                </On>
              </Join>
            </Joins>
          </QueryDefinition>
        </rd:DesignerState>
      </Query>
