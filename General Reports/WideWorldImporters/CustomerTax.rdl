  <DataSets>
    <DataSet Name="CustomerTax_DS">
      <Query>
        <DataSourceName>WideWorldImportersDW_DS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CalendarYear">
            <Value>=Parameters!CalendarYear.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
	city.City,
	city.[State Province],
	cust.Customer,
	dte.[Month],
	dte.[Calendar Month Number],
	ord.Quantity,
	ord.[Total Excluding Tax]
FROM
	Fact.[Order] AS ord
	INNER JOIN Dimension.City		AS city		ON ord.[City Key] = city.[City Key]
	INNER JOIN Dimension.Customer	AS cust		ON ord.[Customer Key] = cust.[Customer Key]
	INNER JOIN Dimension.[Date]		AS dte		ON ord.[Order Date Key] = dte.[Date]
WHERE
	dte.[Calendar Year] = @CalendarYear;</CommandText>
      </Query>
      <Fields>
        <Field Name="City">
          <DataField>City</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="State_Province">
